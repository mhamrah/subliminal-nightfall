  "keywords": [
    "theme",
    "color-scheme",
    "vscode",
    "neovim",
    "zed",
    "ghostty",
    "dark-theme"
  ],
  "author": "Michael Hamrah",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mhamrah/subliminal-nightfall.git"
